    "definition": {
        "Comment": "A Hello World example of the Amazon States Language using a Choice state",
        "StartAt": "Hello",
        "States": {
            "Hello": {
                "Type": "Choice",
                "Choices": [
                  {
                    "NumericLessThan": 1,
                    "Variable": "$.first",
                    "Next": "pathForFirst"
                  },
                  {
                    "NumericLessThan": 0,
                    "Variable": "$.second",
                    "Next": "pathForSecond"
                  },
                  {
                    "NumericLessThan": -1,
                    "Variable": "$.third",
                    "Next": "pathForThird"
                  },
                  {
                    "NumericLessThan": 1.1,
                    "Variable": "$.fourth",
                    "Next": "pathForFourth"
                  },
                  {
                    "NumericLessThanPath": "$.fifthRule",
                    "Variable": "$.fifthVariable",
                    "Next": "pathForFifth"
                  }
                ],
                "Default": "fail"
              },
            "pathForFirst": {
                "Type": "Pass",
            "End": true
            },
            "pathForSecond": {
            "Type": "Succeed"
            },
          "pathForThird": {
            "Type": "Succeed"
            },
           "pathForFourth": {
            "Type": "Succeed"
            },
          "pathForFifth": {
            "Type": "Succeed"
            },
            "fail": {
                "Type": "Fail",
                "Error": "myError",
                "Cause": "myCause"
            }
        }
    } 
