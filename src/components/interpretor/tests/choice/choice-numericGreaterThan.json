    "definition": {
        "Comment": "A Hello World example of the Amazon States Language using a Choice state",
        "StartAt": "Hello",
        "States": {
            "Hello": {
                "Type": "Choice",
                "Choices": [
                  {
                    "NumericGreaterThan": 1,
                    "Variable": "$.first",
                    "Next": "pathForFirst"
                  },
                  {
                    "NumericGreaterThan": 0,
                    "Variable": "$.second",
                    "Next": "pathForSecond"
                  },
                  {
                    "NumericGreaterThan": -1,
                    "Variable": "$.third",
                    "Next": "pathForThird"
                  },
                  {
                    "NumericGreaterThan": 1.1,
                    "Variable": "$.fourth",
                    "Next": "pathForFourth"
                  },
                  {
                    "NumericGreaterThanPath": "$.fifthPath",
                    "Variable": "$.fifth",
                    "Next": "pathForFifth"
                  }
                ],
                "Default": "fail"
              },
            "pathForFirst": {
                "Type": "Pass",
            "End": true
            },
            "pathForSecond": {
            "Type": "Succeed"
            },
          "pathForThird": {
            "Type": "Succeed"
            },
           "pathForFourth": {
            "Type": "Succeed"
            },
          "pathForFifth": {
            "Type": "Succeed"
            },
            "fail": {
                "Type": "Fail",
                "Error": "myError",
                "Cause": "myCause"
            }
        }
    } 
