{
    "describe": "simple state machine with a parallel state and a resultPath", 
    "tests": [
        {
            "describe": "should work and create a 'result' key in the output",
            "input": {"heartbeat": 1, "timeout": 1},
            "expectedStateMachineStatus": "SUCCEEDED",
            "expectedOutput": [{"heartbeat": 1, "timeout": 1, "result": { "Output": 42 } } ],
            "activitiesToCreate": [
                {
                    "name": "tmp",
                    "expectedInput": {"heartbeat": 1, "timeout": 1},
                    "output": {"Output": 42}
                }
            ],
            "parallelEvents": [
                {
                 "timestamp": "2020-12-08T17:55:03.351Z",
                 "type": "ExecutionStarted",
                 "id": 1,
                 "previousEventId": 0,
                 "executionStartedEventDetails": {
                  "input": "{\"heartbeat\":1,\"timeout\":1}",
                  "roleArn": "todo"
                 }
                },
                {
                 "timestamp": "2020-12-08T17:55:03.385Z",
                 "type": "ParallelStateEntered",
                 "id": 2,
                 "previousEventId": 0,
                 "stateEnteredEventDetails": {
                  "name": "Parallel State",
                  "input": "{\"heartbeat\":1,\"timeout\":1}"
                 }
                },
                {
                 "timestamp": "2020-12-08T17:55:03.385Z",
                 "type": "ParallelStateStarted",
                 "id": 3,
                 "previousEventId": 2
                },
                {
                 "timestamp": "2020-12-08T17:55:03.503Z",
                 "type": "TaskStateEntered",
                 "id": 4,
                 "previousEventId": 3,
                 "stateEnteredEventDetails": {
                  "name": "First Task",
                  "input": "{\"heartbeat\":1,\"timeout\":1}"
                 }
                },
                {
                 "timestamp": "2020-12-08T17:55:03.503Z",
                 "type": "ActivityScheduled",
                 "id": 5,
                 "previousEventId": 4,
                 "activityScheduledEventDetails": {
                  "resource": "arn:aws:states:eu-west:012345678901:activity:tmp",
                  "input": "{\"heartbeat\":1,\"timeout\":1}",
                  "timeoutInSeconds": 1,
                  "heartbeatInSeconds": 1
                 }
                },
                {
                 "timestamp": "2020-12-08T17:55:03.596Z",
                 "type": "ActivityStarted",
                 "id": 6,
                 "previousEventId": 5,
                 "activityStartedEventDetails": {}
                },
                {
                 "timestamp": "2020-12-08T17:55:03.786Z",
                 "type": "ActivitySucceeded",
                 "id": 7,
                 "previousEventId": 6,
                 "activitySucceededEventDetails": {
                  "output": "{\"Output\":42}"
                 }
                },
                {
                 "timestamp": "2020-12-08T17:55:03.786Z",
                 "type": "TaskStateExited",
                 "id": 8,
                 "previousEventId": 7,
                 "stateExitedEventDetails": {
                  "name": "First Task",
                  "output": "{\"heartbeat\":1,\"timeout\":1,\"result\":{\"Output\":42}}"
                 }
                },
                {
                 "timestamp": "2020-12-08T17:55:03.786Z",
                 "type": "ParallelStateSucceeded",
                 "id": 9,
                 "previousEventId": 8
                },
                {
                 "timestamp": "2020-12-08T17:55:03.786Z",
                 "type": "ParallelStateExited",
                 "id": 10,
                 "previousEventId": 8,
                 "stateExitedEventDetails": {
                  "name": "Parallel State",
                  "output": "[{\"heartbeat\":1,\"timeout\":1,\"result\":{\"Output\":42}}]"
                 }
                },
                {
                 "timestamp": "2020-12-08T17:55:03.786Z",
                 "type": "ExecutionSucceeded",
                 "id": 11,
                 "previousEventId": 10,
                 "executionSucceededEventDetails": {
                  "output": "[{\"heartbeat\":1,\"timeout\":1,\"result\":{\"Output\":42}}]"
                 }
                }
            ]
        },
