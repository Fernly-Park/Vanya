    "definition": {
        "Comment": "Simple parallel state machine",
        "StartAt": "Parallel State",
        "States": {
          "Parallel State": {
            "Type": "Parallel",
            "Branches": [
              {
                "StartAt": "Parallel 1",
                "States": {
                  "Parallel 1": {
                    "Type": "Parallel",
                    "Branches": [
                      {
                        "StartAt": "Wait 1",
                        "States": {
                          "Wait 1": {
                            "Type": "Wait",
                            "SecondsPath": "$.seconds",
                            "End": true
                          }
                        }
                      }
                    ],
                    "End": true
                  }
                }
              },
              {
                "StartAt": "Parallel 2",
                "States": {
                  "Parallel 2": {
                    "Type": "Parallel",
                    "Branches": [
                      {
                        "StartAt": "Task State 2",
                        "States": {
                          "Task State 2": {
                            "Type": "Task",
                            "Resource": "arn:aws:states:eu-west:012345678901:activity:tmp",
                            "HeartbeatSecondsPath": "$.heartbeat",
                            "TimeoutSecondsPath": "$.timeout",
                            "End": true
                          }
                        }
                      }
                    ],
                    "End": true
                  }
                }
              },
              {
                "StartAt": "Parallel 3",
                "States": {
                  "Parallel 3": {
                    "Type": "Parallel",
                    "Branches": [
                      {
                        "StartAt": "Choice 1",
                        "States": {
                          "Choice 1": {
                            "Type": "Choice",
                            "Choices": [
                              {
                                "Variable": "$.succeed",
                                "BooleanEquals": true,
                                "Next": "Succeeded"
                              }
                            ],
                            "Default": "Failed"
                          },
                          "Succeeded": {
                            "Type": "Succeed"
                          },
                          "Failed": {
                            "Type": "Fail"
                          }
                        }
                      }
                    ],
                    "End": true
                  }
                }
              },
              {
                "StartAt": "Pass State 3",
                "States": {
                  "Pass State 3": {
                    "Type": "Pass",
                    "Result": "third",
                    "End": true
                  }
                }
              }
            ],
            "Next": "NEXT_STATE"
          },
          "NEXT_STATE": {
            "Type": "Pass",
            "End": true
          }
        }
    }
